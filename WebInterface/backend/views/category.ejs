<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Category</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <h1>Category</h1>
  <table>
    <thead>
      <tr>
        <th>CategoryID</th>
        <th>CategoryName</th>
        <!-- Add more columns if needed -->
      </tr>
    </thead>
    <tbody>
      <% category.forEach(cat => { %>
        <tr>
          <td><%= cat.CategoryID %></td>
          <td><%= cat.CategoryName %></td>
          <!-- Add more columns if needed -->
        </tr>
      <% }) %>
    </tbody>
  </table>
  <button onclick="window.location.href='/'">Go Back</button>
</body>
</html>